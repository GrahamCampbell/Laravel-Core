{
    "name": "graham-campbell/core",
    "description": "Core Provides Some Extra Functionality For Laravel 4.1",
    "keywords": ["laravel", "framework", "base", "starter", "core", "Core", "Laravel Core", "Laravel-Core", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.1.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\Core\\Controllers": "src/",
            "GrahamCampbell\\Core\\Models": "src/",
            "GrahamCampbell\\Core\\Providers": "src/",
            "GrahamCampbell\\Core\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

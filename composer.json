{
    "name": "graham-campbell/core",
    "description": "Core Provides Some Extra Functionality For Laravel 5",
    "keywords": ["laravel", "framework", "base", "starter", "core", "Core", "Laravel Core", "Laravel-Core", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "5.0.*|5.1.*",
        "illuminate/contracts": "5.0.*|5.1.*",
        "illuminate/support": "5.0.*|5.1.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.1",
        "laravelcollective/html": "5.0.*|5.1.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Core\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

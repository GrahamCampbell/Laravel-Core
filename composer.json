{
    "name": "graham-campbell/core",
    "description": "Core Provides Some Extra Functionality For Laravel 5",
    "keywords": ["laravel", "framework", "base", "starter", "core", "Core", "Laravel Core", "Laravel-Core", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BootstrapCMS/asset"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/console": "~5.0",
        "illuminate/contracts": "~5.0",
        "illuminate/support": "~5.0",
        "orchestra/config": "~3.0",
        "orchestra/support-providers": "~3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0",
        "illuminate/html": "~5.0",
        "lightgear/asset": "1.5.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Core\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
